{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "ApiGateway",
        "Uri": "/health"
      }
    ],
    "EvaluationTimeOnSeconds": 10,
    "MaximumHistoryEntriesPerEndpoint": 60,
    "StorageMode": "InMemory"
  },
  "ServiceRegistration": {
    "ServiceDiscoveryAddress": "http://localhost:8500"
  }
}
